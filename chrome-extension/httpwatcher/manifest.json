{
  "manifest_version": 2,
  "name": "Http Watcher",
  "description": "Http请求观察者",
  "version": "1.0",
  "permissions": [
    "cookies",
    "tabs",
    "*://*/*",
    "webRequestBlocking",
    "webRequest"
  ],
  "browser_action": {
    "default_icon": "cookie.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}